{
  "name": "gmaps-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "build:gh": "ng build --configuration=ghpages",
    "build:django": "ng build --configuration=django",
    "gen:jobs": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' ./scripts/generate-jobs-spain.ts",
    "test": "ng test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/google-maps": "^20.2.4",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@googlemaps/markerclusterer": "^2.6.2",
    "@primeuix/themes": "^1.2.3",
    "@turf/bbox": "^7.2.0",
    "@turf/helpers": "^7.2.0",
    "@turf/points-within-polygon": "^6.5.0",
    "@turf/simplify": "^3.5.2",
    "primeng": "^20.1.2",
    "rxjs": "~7.8.0",
    "tailwindcss-primeui": "^0.6.1",
    "terra-draw": "^1.15.0",
    "terra-draw-google-maps-adapter": "^1.0.4",
    "tslib": "^2.3.0",
    "turf-simplify": "^3.0.12",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.2",
    "@angular/cli": "^20.3.2",
    "@angular/compiler-cli": "^20.3.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/google.maps": "^3.58.1",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "typescript": "~5.9.2"
  }
}
